<?xml version='1.0' encoding='utf-8'?>
<widget id="com.core.mobil.dg" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Daniela Garcia</name>
    <description>
         Aplicaci√≥n de la Senadora Daniela Garcia.
    </description>
   <author email="leolepez@gmail.com" href="http://grupocore.net">
        Grupo Core - Desarrollado por Leonardo Lepez
    </author>
    <content src="principal.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/icon.png" />
</widget>
