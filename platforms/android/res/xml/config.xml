<?xml version='1.0' encoding='utf-8'?>
<widget id="com.core.mobil.dg" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Daniela Garcia</name>
    <description>
         Aplicaci√≥n de la Senadora Daniela Garcia.
    </description>
    <author email="leolepez@gmail.com" href="http://grupocore.net">
        Grupo Core - Desarrollado por Leonardo Lepez
    </author>
    <content src="principal.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
